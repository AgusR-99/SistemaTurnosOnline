@page "/profesor/create"



<div class="w-50 mx-auto">
    <button class="btn p-0 mb-3"><span class="oi oi-arrow-left"></span></button>
    <h3 class="mb-3">CrearProfesor</h3>
    <div class="form-floating mb-3">
        <input type="number" class="form-control" id="dniInput" placeholder="1234">
        <label for="dniInput">DNI</label>
    </div>

    <div class="form-floating mb-3">
        <input type="text" class="form-control" id="nombreInput" placeholder="name">
        <label for="nombreInput">Nombre y Apellido</label>
    </div>

    <div class="form-floating mb-3">
        <input type="email" class="form-control" id="emailInput" placeholder="name@example.com">
        <label for="emailInput">Direccion Email</label>
    </div>

    <div class="form-floating mb-3">
        <input type="password" class="form-control" id="passwordInput" placeholder="password">
        <label for="passwordInput">Contraseña</label>
    </div>

    <div class="form-floating mb-3">
        <input type="password" class="form-control" id="passwordInputRe" placeholder="password">
        <label for="passwordInputRe">Repetir contraseña</label>
    </div>

    <button class="btn btn-success float-end w-100">Crear</button>
</div>

